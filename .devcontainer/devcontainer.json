{
    "name": "Financial Freedom",
    "dockerComposeFile": [
        "../docker-compose.yml",
        "../docker-compose.dev.yml"
    ],
    "service": "php",
    "workspaceFolder": "/var/www/html",
    "forwardPorts": [80, 443, 8080, 3306],
    "postCreateCommand": [
        "cp .env.example .env",
        "php composer install --optimize-autoloader --no-interaction --no-progress --no-ansi",
    ]
}